                                        "d":"gene_11_14"
                                    }
                                ],
                                "J":""
                            },
                            {  
                                "B":"#text",
                                "C":"/4/2/35",
                                "J":"\n"
                            },
                            {  
                                "B":"p",
                                "C":"/4/2/36",
                                "A":[  
                                    {  
                                        "T":"class",
                                        "d":"ext-hanging"
                                    }
                                ],
                                "J":"<span class=\"vn\">14</span>\u00A0When Shelah was 30\u00A0years old, he be\u00ADcame the fa\u00ADther of Eber. <a id=\"gene_11_15\"></a><span class=\"vn\">15</span>\u00A0Af\u00ADter the birth of Eber, Shelah lived an\u00ADoth\u00ADer 403\u00A0years and had oth\u00ADer sons and daugh\u00ADters."
                            },
                            {  
                                "B":"#text",
                                "C":"/4/2/37",
                                "J":"\n"
                            },
                            {  
                                "B":"a",
                                "C":"/4/2/38[gene_11_16]",
                                "A":[  
                                    {  
                                        "T":"id",
                                        "d":"gene_11_16"
                                    }
                                ],
                                "J":""
                            },
                            {  
                                "B":"#text",
