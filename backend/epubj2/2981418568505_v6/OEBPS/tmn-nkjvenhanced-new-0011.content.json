                                                "A":[  
                                                    {  
                                                        "T":"class",
                                                        "d":"sl"
                                                    }
                                                ],
                                                "J":"And flesh of my flesh;"
                                            },
                                            {  
                                                "B":"#text",
                                                "C":"/4/2/2[ch01002]/52/5",
                                                "J":"\n"
                                            },
                                            {  
